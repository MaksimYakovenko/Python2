def get_number_of_digits(string):
    count = 0
    for char in string:
        try:
            count += 1
        except ValueError:
            pass
    return count
if __name__ == '__main__':
    string = '23123123' #кількість
    print(get_number_of_digits(string))



def sum_digits(string):
    s = 0
    for char in string:
        try:
            n = int(char)
            s += n
        except ValueError:
            pass
    return s


if __name__ == '__main__':
    string = "134314314 " #сума
    print(sum_digits(string))



def chastka_of_digits(string):
    for char in string:
        try:
            n = int(char)
        except ValueError:
            pass
        return n
if __name__ == '__main__':
    string = '458205825'
